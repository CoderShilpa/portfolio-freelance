{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
 
    {
      "src": "/(.*)\\.(css|js|jpg|png|svg|ico)",
      "dest": "/$1.$2"
    },
   
    {
      "src": "/send-email",
      "dest": "server.js"
    },
  
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
