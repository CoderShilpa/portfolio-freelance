{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    
    {
      "src": "/send-email",
      "dest": "server.js",
      "methods": ["POST"]
    },

    {
      "src": "/(.*)\\.(css|js|jpg|png|svg|ico)",
      "dest": "/$1.$2" 
    },
  
    {
      "src": "/",
      "dest": "/index.html"
    },
   
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
