{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "css/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/send-email",
      "dest": "server.js"
    },
    {
      "src": "/(.*)\\.(css|js|jpg|png|svg)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}
